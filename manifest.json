{
  "name": "Connect Ticker",
  "description": "Save yous from re-ticking the Connect T&C every time - once is enough!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://login.det.wa.edu.au/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}